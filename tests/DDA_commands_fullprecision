ddscatcli -CMDSOL QMRCCG -CALPHA LATTDR -MEM_ALLOW "16 16 16" -CSHAPE ELLIPSOID -SHPAR "16 16 16" -DIEL "diel/m1.302265942870714_0.07678923076154483" -WAVELENGTHS "0.5 0.5 1 'LIN'" -AEFF "0.1 0.1 1 'LIN'" -TOL 1.e-4 -IORTH 1 -ROT_BETA "90 90 1" -ROT_THETA "90 90 1" -ROT_PHI "90 90 1" -NRFLD 0 -run
ddscatcli -CMDSOL QMRCCG -CALPHA LATTDR -MEM_ALLOW "16 16 16" -CSHAPE ELLIPSOID -SHPAR "16 16 16" -DIEL "diel/m1.302265942870714_0.07678923076154483" -WAVELENGTHS "0.5 0.5 1 'LIN'" -AEFF "0.1 0.1 1 'LIN'" -TOL 1.e-4 -IORTH 1 -ROT_BETA "90 90 1" -ROT_THETA "90 90 1" -ROT_PHI "90 90 1" -NRFLD 0 -run
